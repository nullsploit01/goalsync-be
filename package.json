  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "export NODE_ENV=local && ts-node-dev src/index.ts --respawn --transpileOnly",
    "deploy": "firebase deploy --only \"functions,hosting\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullsploit01/goalsync-be.git"
  },
  "author": "nullsploit01",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nullsploit01/goalsync-be/issues"
  },
  "engines": {
    "node": "18"
  },
  "homepage": "https://github.com/nullsploit01/goalsync-be#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "firebase-functions-test": "^3.1.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "express-winston": "^4.2.0",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0",
    "winston": "^3.11.0"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint \"./src/**/*{js,ts}\" --fix",
      "prettier --write \"./**/**/*{js,ts}\""
    ]
  }
}
